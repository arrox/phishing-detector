{
  "dashboard": {
    "id": null,
    "title": "Phishing Detector - Observabilidad Operacional",
    "tags": ["phishing-detector", "security", "api"],
    "timezone": "UTC",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "üéØ SLO Overview - Estado General del Servicio",
        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{service=\"phishing-detector\"}",
            "legendFormat": "Servicio Activo"
          },
          {
            "expr": "histogram_quantile(0.95, rate(phishing_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "Latencia P95 (ms)"
          },
          {
            "expr": "rate(phishing_requests_total{status!~\"5..\"}[5m]) / rate(phishing_requests_total[5m]) * 100",
            "legendFormat": "Disponibilidad (%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "üìä Request Rate - Tr√°fico de Requests",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "rate(phishing_requests_total[1m]) * 60",
            "legendFormat": "Total Requests/min"
          },
          {
            "expr": "rate(phishing_requests_total{endpoint=\"/classify\"}[1m]) * 60",
            "legendFormat": "Clasificaciones/min"
          },
          {
            "expr": "rate(phishing_requests_total{status=~\"5..\"}[1m]) * 60",
            "legendFormat": "Errores 5xx/min"
          }
        ],
        "yAxes": [
          {
            "label": "Requests por minuto",
            "min": 0
          }
        ],
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      {
        "id": 3,
        "title": "‚ö° Latency Distribution - Distribuci√≥n de Latencia",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(phishing_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(phishing_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(phishing_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "P99"
          }
        ],
        "yAxes": [
          {
            "label": "Latencia (ms)",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 2000,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 4,
        "title": "üîç Clasificaciones por Tipo - An√°lisis de Resultados",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 14},
        "targets": [
          {
            "expr": "increase(phishing_classifications_total{classification=\"safe\"}[1h])",
            "legendFormat": "‚úÖ Seguros"
          },
          {
            "expr": "increase(phishing_classifications_total{classification=\"suspicious\"}[1h])",
            "legendFormat": "‚ö†Ô∏è Sospechosos"
          },
          {
            "expr": "increase(phishing_classifications_total{classification=\"phishing\"}[1h])",
            "legendFormat": "üö® Phishing"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 5,
        "title": "üõ°Ô∏è Security Events - Eventos de Seguridad",
        "type": "table",
        "gridPos": {"h": 8, "w": 16, "x": 8, "y": 14},
        "targets": [
          {
            "expr": "increase(phishing_errors_total{error_type=\"authentication_failed\"}[1h])",
            "legendFormat": "Fallos de Autenticaci√≥n"
          },
          {
            "expr": "increase(phishing_errors_total{error_type=\"invalid_request\"}[1h])",
            "legendFormat": "Requests Inv√°lidos"
          },
          {
            "expr": "increase(phishing_errors_total{error_type=\"rate_limit_exceeded\"}[1h])",
            "legendFormat": "Rate Limit Superado"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {}
            }
          }
        ]
      },
      {
        "id": 6,
        "title": "üíæ Resource Usage - Uso de Recursos del Contenedor",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{container=\"phishing-detector\"}[5m]) * 100",
            "legendFormat": "CPU Usage (%)"
          },
          {
            "expr": "container_memory_usage_bytes{container=\"phishing-detector\"} / 1024 / 1024 / 1024",
            "legendFormat": "Memory Usage (GB)"
          }
        ],
        "yAxes": [
          {
            "label": "Usage",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 80,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 7,
        "title": "üîß Gemini API Integration - Estado de Integraci√≥n",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "rate(gemini_api_requests_total[5m]) * 60",
            "legendFormat": "Requests/min a Gemini"
          },
          {
            "expr": "rate(gemini_api_errors_total[5m]) * 60",
            "legendFormat": "Errores/min Gemini"
          },
          {
            "expr": "histogram_quantile(0.95, rate(gemini_api_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "Latencia Gemini P95 (ms)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 5000}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "üìà Error Rate Analysis - An√°lisis de Tasas de Error",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "rate(phishing_requests_total{status=\"400\"}[5m]) * 100",
            "legendFormat": "4xx Client Errors (%)"
          },
          {
            "expr": "rate(phishing_requests_total{status=~\"5..\"}[5m]) * 100",
            "legendFormat": "5xx Server Errors (%)"
          },
          {
            "expr": "rate(phishing_errors_total{error_type=\"internal_error\"}[5m]) * 100",
            "legendFormat": "Internal Errors (%)"
          },
          {
            "expr": "rate(phishing_errors_total{error_type=\"gemini_api_error\"}[5m]) * 100",
            "legendFormat": "Gemini API Errors (%)"
          }
        ],
        "yAxes": [
          {
            "label": "Error Rate (%)",
            "min": 0,
            "max": 100
          }
        ],
        "alert": {
          "conditions": [
            {
              "query": {"queryType": "", "refId": "A"},
              "reducer": {"type": "last", "params": []},
              "evaluator": {"params": [5], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 9,
        "title": "üåê Geographic Distribution - Distribuci√≥n Geogr√°fica",
        "type": "worldmap",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 38},
        "targets": [
          {
            "expr": "increase(phishing_requests_total[1h])",
            "legendFormat": "Requests por Pa√≠s"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 10,
        "title": "üìù Recent Logs - Logs Recientes (Cr√≠ticos)",
        "type": "logs",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 38},
        "targets": [
          {
            "expr": "{service=\"phishing-detector\"} |= \"ERROR\" or \"CRITICAL\" or \"security\" or \"authentication\"",
            "legendFormat": ""
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": true,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      },
      {
        "id": 11,
        "title": "‚è±Ô∏è Response Time Heatmap - Mapa de Calor de Latencias",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 46},
        "targets": [
          {
            "expr": "increase(phishing_request_duration_seconds_bucket[1m])",
            "legendFormat": "{{le}}"
          }
        ],
        "heatmap": {
          "hideZeroBuckets": true,
          "highlightCards": true,
          "highlightInSeries": true
        },
        "tooltip": {
          "show": true,
          "showHistogram": true
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "-- Grafana --",
          "type": "dashboard",
          "iconColor": "blue",
          "tags": ["deployment"],
          "limit": 100
        },
        {
          "name": "Security Events", 
          "datasource": "prometheus",
          "type": "alert",
          "iconColor": "red",
          "tags": ["security"],
          "limit": 100
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "query",
          "query": "label_values(phishing_requests_total, environment)",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "includeAll": true
        },
        {
          "name": "instance",
          "type": "query", 
          "query": "label_values(phishing_requests_total{environment=\"$environment\"}, instance)",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "includeAll": true
        }
      ]
    },
    "version": 1,
    "description": "Dashboard completo de observabilidad para el servicio Phishing Detector. Incluye SLOs, m√©tricas de negocio, recursos, seguridad y an√°lisis geogr√°fico."
  }
}